{
  "name": "react-native-onyx",
  "version": "1.0.3",
  "author": "Expensify, Inc.",
  "homepage": "https://expensify.com",
  "description": "State management for React Native",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "expensify-common": "git+https://git@github.com:Expensify/expensify-common.git#5002f0a429e8c62bfc9daa7cbe35864784e67407",
    "lodash.merge": "^4.6.2",
    "react": "^17.0.1",
    "underscore": "^1.11.0"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^3.4.2",
    "@testing-library/react-native": "^7.0.2",
    "babel-jest": "^26.2.2",
    "eslint": "^7.6.0",
    "eslint-config-expensify": "^2.0.11",
    "jest": "^26.5.2",
    "jest-cli": "^26.5.2"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native)/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/tests/unit/mocks/"
    ],
    "testMatch": [
      "**/tests/unit/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "globals": {
      "__DEV__": true,
      "WebSocket": {}
    },
    "timers": "fake",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ]
  }
}
